<service>
  <!-- ID of the service. It should be unique accross the Windows system-->
  <id>smarter</id>
  <!-- Display name of the service -->
  <name>smarter</name>
  <!-- Service description -->
  <description>BI platform</description>
  <!-- Path to the executable, which should be started -->
  <executable>java</executable>
  <arguments>-jar smarter.jar -Dspring.config.location=./application.yaml</arguments>
</service>
